
<navbar activeName='{{activeName}}'>
  <van-icon name="setting-o" bindtap="openSeting"/>
</navbar>

<scroll-view scroll-y="true" class="main-content" refresher-triggered="{{refreStatus}}" refresher-enabled="true" bindrefresherrefresh="handleRefresh">
  <block wx:if="{{active === 0}}">
    <appoint  bind:parentEvent='onChangeTabs'/>
  </block>
  <block wx:if="{{active > 0}}">
    <history  id="history"/>
  </block>
</scroll-view>

<van-tabbar active="{{ active }}" bind:change="onChangeTabs">
  <van-tabbar-item icon="home-o">预约</van-tabbar-item>
  <van-tabbar-item icon="search">历史</van-tabbar-item>
</van-tabbar>

